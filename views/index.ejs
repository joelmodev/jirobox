                  <div class="col">
                   <div class="p-3"><div class="mb-3" id="status">
                    
                   </div>
                 <div class="mb-3">
                     <label for="exampleFormControlInput1" class="form-label">Email</label>
                     <input type="email" id="form-email" class="form-control" id="exampleFormControlInput1" name="email" placeholder="name@example.com" required>
